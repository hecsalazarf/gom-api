{
  "name": "gom-api",
  "version": "0.2.0",
  "description": "Gom Api for Gom App (https://github.com/hecsalazarf/gom-ui)",
  "author": "HÃ©ctor Salazar <hecsalazarf@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npx copyfiles -E -f src/db/prisma/prisma-types.graphql dist/db/prisma/",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:deploy": "prisma deploy",
    "graphql:schema": "graphql get-schema",
    "graphql:binding": "graphql codegen",
    "graphql:typings": "ts-node generate-graphql-typings",
    "graphql:build": "npm run prisma:deploy && npm run graphql:schema && npm run graphql:binding && npm run graphql:typings"
  },
  "dependencies": {
    "@casl/ability": "^3.2.0",
    "@nestjs/common": "^6.6.7",
    "@nestjs/core": "^6.6.7",
    "@nestjs/graphql": "^6.5.0",
    "@nestjs/platform-express": "^6.6.7",
    "apollo-server-express": "^2.9.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "config": "^3.2.2",
    "connect-redis": "^4.0.2",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "csurf": "^1.10.0",
    "express-session": "^1.16.2",
    "helmet": "^3.20.1",
    "ioredis": "^4.14.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "prisma-binding": "^2.3.15",
    "rate-limiter-flexible": "^1.1.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "web-push": "^3.3.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.6.7",
    "@types/config": "0.0.34",
    "@types/connect-redis": "0.0.11",
    "@types/cookie": "^0.3.3",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/ioredis": "^4.0.16",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/node": "^12.7.4",
    "@types/supertest": "^2.0.7",
    "@types/web-push": "^3.3.0",
    "concurrently": "^4.1.2",
    "copyfiles": "^2.1.1",
    "graphql-cli": "^3.0.14",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.15.3",
    "prisma": "^1.34.8",
    "supertest": "^4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
