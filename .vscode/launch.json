  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Nestjs debug",
      "port": 9229,
      "restart": true,
      // "preLaunchTask": "nestjs:debug",
      // "stopOnEntry": false,
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Nestjs debug",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [ "run", "start:debug" ],
      "protocol": "auto",
      "restart": true,
      "port": 9229,
      "autoAttachChildProcesses": true,
      
    }
  ]
}