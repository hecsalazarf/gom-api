{
  "name": "gom-api",
  "version": "0.4.1",
  "description": "Gom Api for Gom App (https://github.com/hecsalazarf/gom-ui)",
  "author": "HÃ©ctor Salazar <hecsalazarf@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npx copyfiles -E -f src/db/prisma/prisma-types.graphql dist/db/prisma/",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --inspect-brk=22525 --config nodemon-debug.json src/main.ts",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "eslint --ext .js,.ts src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:deploy": "prisma deploy",
    "graphql:schema": "graphql get-schema",
    "graphql:binding": "graphql codegen",
    "graphql:typings": "ts-node generate-graphql-typings",
    "graphql:build": "npm run prisma:deploy && npm run graphql:schema && npm run graphql:binding && npm run graphql:typings"
  },
  "dependencies": {
    "@casl/ability": "^3.3.0",
    "@nestjs/common": "^6.10.10",
    "@nestjs/core": "^6.10.10",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/platform-express": "^6.10.10",
    "apollo-server-express": "^2.9.13",
    "bullmq": "^1.6.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "config": "^3.2.4",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "csurf": "^1.10.0",
    "express-session": "^1.17.0",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "prisma-binding": "^2.3.16",
    "rate-limiter-flexible": "^1.1.6",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "web-push": "^3.4.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.10.10",
    "@types/config": "0.0.36",
    "@types/connect-redis": "0.0.13",
    "@types/cookie": "^0.3.3",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/ioredis": "^4.14.1",
    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.17",
    "@types/supertest": "^2.0.7",
    "@types/web-push": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "concurrently": "^5.0.1",
    "copyfiles": "^2.1.1",
    "eslint": "^6.7.2",
    "graphql-cli": "^3.0.14",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "prisma": "^1.34.10",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
